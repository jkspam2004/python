# reverse the integers in an array
# first line of input is the length of the array
# 4
# 1 4 3 2

n=input()

integers=input().split(" ")

i=int(n)
new = ''
while i > 0:
    i-=1
    new += integers[i] + ' '

print(new)    



# or newlist and join to a new string later


i=int(n)
newlist = []
while i > 0:
    i-=1
    #print(int(integers[i]))
    new += integers[i] + ' '
    newlist.append(integers[i])

newstr = ' '.join(newlist)
print(newstr)
